{
  "parser": "@typescript-eslint/parser",
  "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
  "rules": {
    "@typescript-eslint/no-explicit-any": 0,
    "@typescript-eslint/no-unused-vars": "warn"
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".ts", ".d.ts"],
        "moduleDirectory": ["src", "types"]
      },
      "typescript": {
        "extensions": [".ts", ".d.ts"],
        "moduleDirectory": ["src", "types"]
      }
    }
  }
}
